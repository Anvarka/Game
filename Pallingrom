def search_pallindrom(stroka):
    exs = stroka
    exs = exs.replace("A", "t")
    exs = exs.replace("T", "a")
    exs = exs.replace("C", "g")
    exs = exs.replace("G", "c")
    exs = exs.upper()[::-1]
    return exs == stroka


s = ""
exs = ""
for i in range(len(s)):
    if len(s) <= i+12:
        for j in range(i+3, len(s), 2):
            exs = s[i:j+1]
            if search_pallindrom(exs):
                print(i+1, end=" ")
                print(j-i+1)
    else:
        for j in range(i+3, i+12, 2):
            exs = s[i:j+1]
            if search_pallindrom(exs):
                print(i+1, end=" ")
                print(j-i+1)
